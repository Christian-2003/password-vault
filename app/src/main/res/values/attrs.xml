<?xml version="1.0" encoding="utf-8"?>

<resources>
    <!-- Attributes used by multiple views -->
    <attr name="text" format="string"/>

    <!-- XML attributes for the ListButtonView -->
    <declare-styleable name="ListButtonView">
        <attr name="text"/>
    </declare-styleable>

    <!-- XML attributes for the CheckboxUiView -->
    <declare-styleable name="CheckboxUiView">
        <attr name="text"/>
        <attr name="checked" format="boolean"/>
    </declare-styleable>

</resources>
