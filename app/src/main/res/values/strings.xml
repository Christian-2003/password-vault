    <!-- Regexes for password quality gates -->
    <string name="quality_gate_regex_0" translatable="false">\\b(\\w|\\W){12,}\\b</string>
    <string name="quality_gate_regex_1" translatable="false">\\b(\\w|\\W){18,}\\b</string>
    <string name="quality_gate_regex_2" translatable="false">(\\w|\\W)*[!"'§$%&amp;/{(\\[})\\]=?\\\\°^*+~#,;.:-_€>&lt;|]+(\\w|\\W)*</string>
    <string name="quality_gate_regex_3" translatable="false">(\\w|\\W)*[0-9]+(\\w|\\W)*</string>
    <string name="quality_gate_regex_4" translatable="false">(\\w|\\W)*[A-Z]+(\\w|\\W)*</string>

